		"@mui/icons-material": "^5.0.1",
		"@mui/material": "^5.0.1",
		"@mui/styles": "^5.0.1",
		"@reduxjs/toolkit": "^1.5.1",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"@types/jest": "^24.0.0",
		"@types/node": "^12.0.0",
		"@types/react": "^16.9.0",
		"@types/react-dom": "^16.9.0",
		"@types/react-redux": "^7.1.7",
		"axios": "^0.21.4",
		"formik": "^2.2.9",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.0",
		"react-router-dom": "^5.3.0",
		"react-scripts": "4.0.3",
		"redux-saga": "^1.1.3",
		"ts-jest": "^27.0.5",
		"ts-node": "^10.2.1",
		"typescript": "~4.1.5"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --passWithNoTests --watchAll=false",
		"eject": "react-scripts eject",
		"prettier-check": "prettier --check src/**/*.ts{,x}",
		"format": "prettier --write src/**/*.ts{,x}",
		"format-modules": "prettier --write src/pages/modules/**/*.ts{,x}",
		"lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
		"lint-fix": "tsc --noEmit && eslint src/**/*.ts{,x} --fix",
		"pre-pr": "npm run format && npm run lint-fix && npm run test && npm run build"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/react-router-dom": "^5.3.0",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-airbnb-typescript": "^14.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-jest": "^24.4.0",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.25.1",
		"eslint-plugin-react-hooks": "^1.7.0",
		"prettier": "^2.3.2"
	}
}
